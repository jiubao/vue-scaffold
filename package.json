    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "vue-loader": "^15.0.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-middleware": "^3.1.3"
  },
  "dependencies": {
    "@jiubao/vue-validator": "^0.0.18",
