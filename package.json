{
  "name": "vue-scaffold",
  "version": "0.0.1",
  "description": "vue webpack express",
  "main": "index.js",
  "scripts": {
    "serve": "node ./build/serve.js",
    "build": "rm -rf dist && mkdir dist && cp ./index.html ./dist/ && npm run dev",
